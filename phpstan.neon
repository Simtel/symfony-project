includes:
    - vendor/phpstan/phpstan-doctrine/extension.neon
rules:
    - Simtel\PHPStanRules\Rule\CommandClassShouldBeHelpCommandHandlerClass
parameters:
    paths:
        - src
    # The level 8 is the highest level
    level: max
    ignoreErrors:
    excludePaths:
    checkMissingIterableValueType: false
