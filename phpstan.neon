includes:
    - vendor/phpstan/phpstan-doctrine/extension.neon
    - vendor/simtel/phpstan-rules/rules.neon
parameters:
    doctrine:
        objectManagerLoader: phpstan-object-manager.php
    paths:
        - src
        - tests
    level: max
    ignoreErrors:
    excludePaths:
    checkMissingIterableValueType: false
